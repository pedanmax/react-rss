{
  "name": "react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "ts-node ./server.ts ",
    "build": "npx eslint src && npx prettier src --check && vite build",
    "preview": "vite preview",
    "lint": "npx eslint src",
    "test": "vitest",
    "cypress": "npx rimraf coverage && npx rimraf .nyc_output && cypress open",
    "test:clean": "npx rimraf coverage && npx rimraf .nyc_output",
    "cypress:screen": "npx rimraf coverage && npx rimraf .nyc_output && npx cypress run --e2e && npx nyc report --reporter=text-summary",
    "ci": "start-server-and-test dev http://localhost:5173 cypress:screen",
    "coverage": "vitest run --coverage",
    "format": "prettier --write .",
    "fix-lint": "eslint --fix --ext .ts,.tsx ."
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "cypress": "^12.10.0",
    "express": "^4.18.2",
    "h3": "^1.6.4",
    "listhen": "^1.0.4",
    "msw": "^1.2.1",
    "nyc": "^15.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.8",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "redux-mock-store": "^1.5.4",
    "sirv": "^2.0.2",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@testing-library/dom": "^9.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.3",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.3",
    "c8": "^7.13.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "react-router-dom": "^6.9.0",
    "rimraf": "^5.0.0",
    "sass": "^1.58.3",
    "start-server-and-test": "^2.0.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.0",
    "vite-plugin-checker": "^0.5.6",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-istanbul": "^4.0.1",
    "vitest": "^0.30.1"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "nyc": {
    "reporter": [
      "html"
    ]
  }
}
